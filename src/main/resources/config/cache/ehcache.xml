<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="app-cache">

    <diskStore path="../temp/app/ehcache" />
	<defaultCache maxEntriesLocalHeap="2000" eternal="false" timeToIdleSeconds="3600" timeToLiveSeconds="0" overflowToDisk="true" />
	<!-- 字典数据缓存  12小时有效 -->
	<cache name="system-dict" maxEntriesLocalHeap="200" eternal="true" timeToLiveSeconds="43200" timeToIdleSeconds="0"/>
</ehcache>
